{
  "name": "configcat-vue",
  "description": "A community-maintained SDK for integrating ConfigCat feature flags into Vue.js applications.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/configcat-vue.umd.cjs",
  "module": "./dist/configcat-vue.js",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/configcat-vue.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/configcat-vue.umd.cjs",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "version": "2.4.0",
  "keywords": [
    "configcat",
    "ConfigCat",
    "configcat vue",
    "configcat vue3",
    "feature-flags",
    "feature flags",
    "feature-toggle",
    "feature toggle",
    "feature-management",
    "feature management",
    "feature-flag",
    "feature flag",
    "feature flags vue",
    "vue3 feature flags",
    "vite-plugin-vue-configcat"
  ],
  "author": "Chavez Harris",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "peerDependencies": {
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@types/node": "^22.4.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^14.12.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "dependencies": {
    "configcat-common": "9.3.1"
  }
}
